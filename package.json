{
  "name": "iarasort",
  "displayName": "iarasort",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "iarasort.sort",
        "title": "Iara: Sort lines (ASCII order)"
      },
      {
        "command": "iarasort.vcpkg_sort",
        "title": "Iara: Sort vcpkg.json file by package names"
      },
      {
        "command": "iarasort.replace_path",
        "title": "Iara: Replace backslashes with forward slashes"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  },
  "extensionDependencies": [
    "esbenp.prettier-vscode"
  ],
  "icon": "./assets/iara.png",
  "repository": {
    "url": "https://github.com/iarasystems/iarasort"
  },
  "publisher": "marcogmaia"
}
